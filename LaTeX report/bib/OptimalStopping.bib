keywords = {Accessibility,Network analysis,PTAI,Population density,Public transport},
pages = {273--285},
publisher = {Elsevier Ltd},
title = {{Public transport accessibility in metropolitan areas: A new approach incorporating population density}},
url = {http://dx.doi.org/10.1016/j.jtrangeo.2016.06.019},
volume = {54},
year = {2016}
}
