journal = {Transport Policy},
keywords = {Accessibility,Affordable housing,Connectivity,Equity,Public transport},
pages = {283--293},
publisher = {Elsevier},
title = {{Equity in transport: The distribution of transit access and connectivity among affordable housing units}},
url = {http://dx.doi.org/10.1016/j.tranpol.2013.09.020},
volume = {30},
year = {2013}
