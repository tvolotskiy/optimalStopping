keywords = {Flanders,GIS,Public transport,Social exclusion,Transport disadvantage,Transport gap},
pages = {176--187},
publisher = {Elsevier B.V.},
title = {{Identifying public transport gaps using time-dependent accessibility levels}},
url = {http://dx.doi.org/10.1016/j.jtrangeo.2015.09.008},
volume = {48},
year = {2015}
}
