	keywords = {Preferred,Smart card data,activity location modelling,public transport,working locations},
	mendeley-groups = {Demand from validations},
	number = {July},
	title = {{Activity Identification and Primary Location Modelling based on Smart Card Payment Data for Public Transport}},
	year = {2012}
}

