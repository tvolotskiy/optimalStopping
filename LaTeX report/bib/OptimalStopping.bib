pages = {27--39},
publisher = {Elsevier Ltd},
title = {{Dynamic transit accessibility and transit gap causality analysis}},
url = {http://dx.doi.org/10.1016/j.jtrangeo.2017.01.006},
volume = {59},
year = {2017}
}
